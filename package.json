{
  "name": "consulta-placa",
  "version": "1.0.0",
  "description": "App para consultar placa de veiculos",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -o server.js --presets es2015,stage-0",
    "start": "node server.js",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "keywords": [
    "placa",
    "veiculos",
    "carro",
    "moto",
    "caminhao",
    "detran"
  ],
  "repository": "https://github.com/wgenial/consulta-placa",
  "author": "WGenial <developers@wgenial.com.br>",
  "license": "MIT",
  "engines": {
    "node": ">= 0.6.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "sinesp-nodejs": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.6.1",
    "nodemon": "^1.12.0"
  }
}
